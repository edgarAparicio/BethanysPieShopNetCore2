@model IEnumerable<EdgarAparicio.BethanysPieShop.Business.Entity.Pie>

<!DOCTYPE html>
<<html>
<head>
    <meta name="viewport" content="width=device-width"/>
</head>
<body>
    <h2>@ViewBag.Title</h2>

    @foreach(var pie in Model)
    {
        <div>
            <h2>@pie.Nombre</h2>
            <h3>@pie.Precio.ToString("c")</h3>
        </div>
    }
</body>
</html>